{
	"name": "react-ssr-netlify-functions",
	"version": "1.0.0",
	"author": "Mikael Vesavuori",
	"description": "React SSR on Netlify Functions (AWS Lambda)",
	"dependencies": {
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"core-js": "3.6.4",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"path": "^0.12.7",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"serverless-http": "^2.3.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-syntax-class-properties": "^7.8.3",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.9.0",
		"@babel/preset-react": "^7.9.1",
		"babel-loader": "^8.1.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.4.2",
		"file-loader": "^6.0.0",
		"html-loader": "^1.0.0",
		"html-webpack-plugin": "^4.0.0",
		"mini-css-extract-plugin": "^0.9.0",
		"netlify-lambda": "^1.6.3",
		"prop-types": "^15.7.2",
		"script-ext-html-webpack-plugin": "^2.1.4",
		"style-loader": "^1.1.3",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2",
		"workbox-webpack-plugin": "^5.1.1"
	},
	"scripts": {
		"start": "webpack-dev-server --config webpack.dev.js --open --mode development",
		"start:server": "netlify-lambda serve functions",
		"build": "webpack --config webpack.prod.js --mode production",
		"build:functions": "yarn build && netlify-lambda build functions",
		"deploy": "yarn build && yarn build:functions && netlify deploy"
	},
	"proxy": {
		"/.netlify/functions": {
			"target": "http://localhost:9000",
			"pathRewrite": {
				"^/\\.netlify/functions": ""
			}
		}
	}
}
